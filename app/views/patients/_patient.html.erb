<div id="<%= dom_id patient %>" class="card h-100">
  <%= link_to patient, class: "text-decoration-none" do %>
    <div class="card-body">
      <h5 class="card-title">
        <strong>Patient name:</strong>
        <%= patient.name %>
      </h5>

      <p class="card-text">
        <strong>Start time:</strong>
        <%= patient.address %>
      </p>

    </div>
  <% end %>
</div>
